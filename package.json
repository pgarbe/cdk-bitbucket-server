{
  "name": "@pgarbe/cdk-bitbucket-server",
  "version": "0.1.0",
  "description": "A CDK Construct library to work with BitBucket Server (Stash)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "test": "jest --group=unit  && awslint",
    "integ": "jest --group=integ",
    "cdk": "cdk",
    "package": "jsii-pacmak",
    "awslint": "awslint"
  },
  "author": {
    "name": "Philipp Garbe",
    "url": "https://garbe.io"
  },
  "awscdkio": {
    "twitter": "pgarbe"
  },
  "repository": {
    "type": "GitHub",
    "url": "https://github.com/pgarbe/cdk-bitbucket-server"
  },
  "license": "MIT",
  "jsii": {
    "outdir": "pack",
    "targets": {
      "python": {
        "distName": "cdk-bitbucket-server",
        "module": "cdk_bitbucket_server"
      }
    }
  },
  "keywords": [
    "cdk",
    "aws-cdk",
    "construct",
    "aws",
    "pgarbe",
    "bitbucket",
    "bitbucket server"
  ],
  "devDependencies": {
    "@aws-cdk/assert": "1.46.0",
    "@types/aws-lambda": "^8.10.56",
    "@types/jest": "^26.0.0",
    "@types/node": "14.0.13",
    "aws-cdk": "1.46.0",
    "aws-sdk": "^2.701.0",
    "awslint": "^1.46.0",
    "jest": "^26.0.1",
    "jest-runner-groups": "^2.0.1",
    "jsii": "^1.7.0",
    "jsii-pacmak": "^1.7.0",
    "pre-commit": "^1.2.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.5"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "^1.46.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.46.0",
    "@aws-cdk/aws-secretsmanager": "^1.46.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.46.0",
    "@aws-cdk/core": "1.46.0",
    "@aws-cdk/custom-resources": "^1.46.0",
    "constructs": "^3.0.3"
  },
  "peerDependencies": {
    "constructs": "^3.0.3",
    "@aws-cdk/aws-iam": "^1.46.0",
    "@aws-cdk/aws-lambda": "^1.46.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.46.0",
    "@aws-cdk/core": "1.46.0",
    "@aws-cdk/custom-resources": "^1.46.0",
    "@aws-cdk/aws-secretsmanager": "^1.46.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.46.0"
  }
}
